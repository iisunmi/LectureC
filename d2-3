#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main1() {
	int cnt = 0, n=2, i;
	for (int v = 1; v < 10; v++) {
		printf("%d", v);
		if (cnt == 3) {
			printf("\n");
			cnt = 0;
		}
		for(n=2;n<10;n++)
		
		for (i = 1; i <= 9; i++) {
			printf("%d*%d=%d\n", n, i, n * i);
		}

		
	}
	return 0;
}
